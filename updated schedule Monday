for each fet in Form[ID != 0]
{
		if(fet.WEEK != null)
		{
		sat = fet.WEEK.addDay(5);
			if(zoho.currentdate > sat)
			{
			fet.WEEK=zoho.currentdate.toStartOfWeek().addDay(1).addDay(7);
			info "In the If Confition";
			// info rec.WEEK + " :: " +rec.ID;

			}

		}

}
///Makes sense, the subform/list as a whole is not null but individual entries are
